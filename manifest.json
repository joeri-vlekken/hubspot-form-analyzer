{
  "manifest_version": 2,

  "name": "HubSpot Form Analyzer",
  "description": "This extension will detect all HubSpot forms and expose all hidden fields.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["override.css"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
